{
    "connector": "local",
    "extends": ["accessibility", "progressive-web-apps", "development"],
    "formatters": ["html", "summary"],
    "hints": {
        "axe": "error",
        "babel-config/is-valid": "error",
        "disown-opener": "error",
        "highest-available-document-mode": "error",
        "manifest-exists": "off",
        "meta-charset-utf-8": "error",
        "meta-viewport": "error",
        "validate-set-cookie-header": "error"
    },
    "hintsTimeout": 10000
}
